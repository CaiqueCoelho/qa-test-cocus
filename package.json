{
  "name": "qa-test-cocus",
  "version": "1.0.0",
  "description": "CaÃ­que Coelho QA Test for COCUS",
  "main": "index.js",
  "scripts": {
    "report:merge": "mochawesome-merge cypress/results/json/*.json > index.json",
    "report:generate": "marge index.json --reportDir public --assetsDir public/assets --reportPageTitle index.html",
    "report:html": "npm run report:merge ; npm run report:generate",
    "report:copyScreenshots": "cp -r cypress/screenshots cypress/results/html/screenshots",
    "test": "npx cypress run --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
    "debug": "npx cypress open",
    "build": "docker-compose build",
    "up": "docker-compose up --abort-on-container-exit --exit-code-from e2e",
    "test:chrome": "cypress run --config pageLoadTimeout=60000,retries=3,failOnStatusCode=false,defaultCommandTimeout=4000 --browser chrome",
    "test:electron": "cypress run --config pageLoadTimeout=60000,retries=3,failOnStatusCode=false,defaultCommandTimeout=4000 --browser electron",
    "test:Flights": "cypress run --config pageLoadTimeout=60000,retries=3,failOnStatusCode=false,defaultCommandTimeout=4000 --spec 'cypress/integration/Flights/Flights.test.js'",
    "test:Login": "cypress run --config pageLoadTimeout=60000,retries=3,failOnStatusCode=false,defaultCommandTimeout=4000 --spec 'cypress/integration/Login/Login.test.js'",
    "test:LoginMobile": "cypress run --config pageLoadTimeout=60000,retries=3,failOnStatusCode=false,defaultCommandTimeout=4000 --spec 'cypress/integration/Login/LoginMobile.test.js'"
  },
  "keywords": [
    "cocus",
    "cypress",
    "qa-test",
    "qa"
  ],
  "author": "Caique Coelho <caiquedpfc@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "axe-core": "^4.3.3",
    "cypress": "^9.5.2",
    "cypress-axe": "^0.13.0",
    "cypress-multi-reporters": "^1.5.0",
    "mocha": "^9.2.1",
    "mochawesome": "^6.3.1",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.1.0"
  }
}
